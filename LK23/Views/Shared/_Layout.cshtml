<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - LK23</title>
    <link rel="stylesheet" href="~/css/styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/LK23/styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link-item" asp-controller="Home" asp-area="" asp-action="Index">About Me</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link-item" asp-controller="Education" asp-area="" asp-action="Index">Education</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link-item" asp-controller="ProfSum" asp-area="" asp-action="Index">Professional Summary</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link-item" asp-controller="Work" asp-area="" asp-action="Index">Work Experience</a>
                </li>
            </ul>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    
</body>
</html>
